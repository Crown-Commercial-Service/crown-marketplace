<% if Marketplace.ga_tracking_id.present? %>
<script async src="https://www.googletagmanager.com/gtag/js?id=<%= Marketplace.ga_tracking_id %>"></script>
<script>
window.dataLayer = window.dataLayer || [];
function gtag(){dataLayer.push(arguments);}
gtag('js', new Date());
gtag('config', '<%= Marketplace.ga_tracking_id %>', { 'anonymize_ip': true });
</script>
<% end %>
