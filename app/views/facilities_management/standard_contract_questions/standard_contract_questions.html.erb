<div class="govuk-body">
  <%= form_tag '/facilities-management/buildings-list', method: :get do %>
    <div class="govuk-grid-row">
      <div class="govuk-grid-column-full">
        <a href="/facilities-management/select-locations" class="govuk-back-link">Back</a>
        <%= render('errors/inline_error_summary') %>
        <%= render('facilities_management/standard_contract_questions/title_section') %>
        <%= render('facilities_management/standard_contract_questions/contract_length') %>
        <%= render('facilities_management/standard_contract_questions/contract_extensions') %>
        <%= render('facilities_management/standard_contract_questions/estimated_contract_value') %>
        <%= render('facilities_management/standard_contract_questions/tupe') %>
      </div>
    </div>
    <!--
    <div class="govuk-grid-row govuk-!-margin-top-4">
      <a id="fm-questions-continue" href="/facilities-management/buildings-list" class="govuk-button ccs-no-print" role="button">Continue</a>
    </div>
    -->
    <div class="govuk-grid-row govuk-!-margin-top-4">
  <%= submit_tag t('common.submit'), class: "govuk-button ccs-no-print"%>
    </div>
  <% end %>
</div>
