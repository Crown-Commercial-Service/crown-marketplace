<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <h2 class="govuk-heading-m govuk-!-margin-bottom-4"><%= t('.upload_progress') %></h2>
    <div class="govuk-body-s ccs-upload-progress-container">
      <div aria-current="true" id="progress-0" class="govuk-!-font-weight-bold"><%= t('.file_uploaded') %></div>
      <div id="progress-1" ><%= t('.checking_file') %></div>
      <div id="progress-2" ><%= t('.processing_data') %></div>
      <div id="progress-3" ><%= t('.publishing_data') %></div>
      <div id="progress-4" ><%= t('.complete') %></div>
    </div>

    <div class="ccs-progress">
      <div id="fm-admin-import-progress" class="ccs-progress-bar" role="progressbar" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100" style="width:10%"></div>
    </div>
  </div>
</div>
