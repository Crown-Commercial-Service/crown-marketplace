<%= content_for :page_title, @building_service.name.html_safe %>
<div class="govuk-body govuk-grid-row procurement-building-service">
    <div class="govuk-grid-column-full">
      <%= link_to 'Back', facilities_management_procurement_building_path(@procurement_building.id), class: 'govuk-back-link govuk-!-margin-top-0 govuk-!-margin-bottom-6' %>
    </div>
    <div class="govuk-grid-column-three-quarters">
      <span class="govuk-caption-m"><%= @procurement_building.procurement.contract_name %>
        - <%= @procurement_building.name %></span>
      <h1 class="govuk-heading-xl"><%= @building_service.name %></h1>
      <%= render partial: "#{@partial_prefix}"%>
  </div>
</div>
