<% @regions.each do |key, value| %>
<div class="govuk-accordion__section ">
  <div class="govuk-accordion__section-header">
    <h2 class="govuk-accordion__section-heading">
      <span class="govuk-accordion__section-button" id="region-<%= key %>"><%= value %></span>
    </h2>
  </div>
  <div id="<% key %>-content" class="govuk-accordion__section-content" aria-labelledby="<%= value %>">

    <% @subregions.each do |key2, value2| %>

      <% if key2.start_with?(key) %>
        <div class="govuk-form-group">
          <fieldset class="govuk-fieldset" aria-describedby="<%= value2 %>">
            <div id="location-checkboxes" class="govuk-checkboxes">
              <div class="govuk-checkboxes__item">
                <input class="govuk-checkboxes__input" id="<%= key2 %>" name="<%= key2 %>" locationid="<%= value2 %>" type="checkbox" value="<%= key2 %>">
                <label id="<%= key2 %>_label" class="govuk-label govuk-checkboxes__label" for="<%= key2 %>">
                  <%= value2 %>
                </label>
              </div>
            </div>
          </fieldset>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
<% end %>
