<span class="govuk-caption-xl"><%= @procurement.contract_name %></span>
<h1 class="govuk-heading-xl"><%= t('.heading') %></h1>
<div class="ccs-font-weight-semi-bold govuk-body">
  Status: <%= govuk_tag_with_text(*@spreadsheet_import.state_to_string) %>
</div>
<% if @spreadsheet_import.importing? %>
  <%= render partial: 'importing' %>
<% elsif @spreadsheet_import.succeeded? %>
  <%= render partial: 'success' %>
<% elsif @spreadsheet_import.failed? %>
  <%= render partial:  'failed' %>
<% else %>
  <p>Something went wrong. Please try again</p>
<% end %>
