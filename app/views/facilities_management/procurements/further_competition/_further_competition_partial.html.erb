            </div>

            <div class="app-step-nav__panel js-panel" id="step-panel-offer-accepted-3">
              <ol class="app-step-nav__list " data-length="4">
                <li class="app-step-nav__list-item js-list-item ">
                  <span data-position="3.1"><%= t('.publishing_invitation_1') %>
                  </span>
                </li>
              </ol>
            </div>
          </li>
          <li class="app-step-nav__step js-step step-is-shown" id="offer-declined">
            <div class="app-step-nav__header js-toggle-panel" data-position="4">
              <h2 class="app-step-nav__title">
              <span class="app-step-nav__circle app-step-nav__circle--number">
                <span class="app-step-nav__circle-inner">
                  <span class="app-step-nav__circle-background">
                    <span class="govuk-visually-hidden">Step</span> 4
                  </span>
                </span>
              </span>

                <span class="js-step-title"><div class="app-step-nav__button app-step-nav__button--title js-step-title-button" aria-expanded="true" aria-controls="step-panel-offer-declined-4"><span class="js-step-title-text">
                  <%= t('.tender_period')%>
              </span><span class="app-step-nav__toggle-link js-toggle-link" aria-hidden="true" hidden="">Hide</span></div></span>
              </h2>
            </div>

            <div class="app-step-nav__panel js-panel" id="step-panel-offer-declined-4">
              <ol class="app-step-nav__list " data-length="3">
                <li class="app-step-nav__list-item js-list-item ">
                  <span data-position="4.1"><%= t('.tender_period_1')%></span>
                </li>
                <li class="app-step-nav__list-item js-list-item ">
                  <span data-position="4.2"><%= t('.tender_period_2')%></span>
                </li>
                <li class="app-step-nav__list-item js-list-item ">
                  <span data-position="4.3"><%= t('.tender_period_3')%> </span>
                </li>
              </ol>
            </div>
