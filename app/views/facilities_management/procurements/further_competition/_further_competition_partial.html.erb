          <ul class="govuk-list govuk-list--bullet">
            <li><%= t('.more_information_1') %></li>
            <li><%= t('.more_information_2') %></li>
            <li><%= t('.more_information_3') %></li>
            <li><%= t('.more_information_4') %></li>
            <li><%= t('.more_information_5') %></li>
          </ul>
    </div>

