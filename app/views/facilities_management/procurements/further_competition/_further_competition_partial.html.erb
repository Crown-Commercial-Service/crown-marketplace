  <div class="">
    <h2 class="govuk-heading-xl govuk-!-font-weight-bold"> <%= t('.summary_header') %></h2>
    <p class="govuk-body"> <%= "Your procurement summary reference number is #{@procurement_reference}"%>.</p>

    <div class="govuk-grid-column-two-thirds govuk-!-padding-bottom-4">
      <details class="govuk-details" data-module="govuk-details">
        <summary class="govuk-details__summary">
          <span class="govuk-details__summary-text">
            <%= t('.description_header') %>
          </span>
        </summary>
        <div class="govuk-details__text ">
          <p class="govuk-body-m"><%= t('.description_text') %></p>

          <ul class="govuk-list govuk-list--bullet govuk-body-m">
            <li><%= t('.description_list_1') %></li>
            <li><%= t('.description_list_2') %></li>
            <li><%= t('.description_list_3') %></li>
            <li><%= t('.description_list_4') %></li>
            <li><%= t('.description_list_5') %></li>
          </ul>
        </div>
      </details>
    </div>

    <div class="govuk-grid-column-two-thirds govuk-!-padding-bottom-4">
      <h3 class="govuk-heading-l">File download</h3>
      <p class="govuk-body-l">Download the procurement summary in XLSX format.</p>
      <a href="<%= facilities_management_procurement_further_competition_spreadsheet_path(@procurement.id) %>?format=xlsx" class="supplier-record__file-download" type="application/msword" download="">
        <%= @page_data[:model_object].contract_name %> procurement summary <span class="govuk-visually-hidden"><abbr title="Microsoft Word">DOC</abbr></span>
      </a>
    </div>
  </div>

  <div class="govuk-grid-column-two-thirds">
    <h2 class="govuk-heading-l"><%= t('.what_happens_next_title') %></h2>
    <p><%= t('.what_happens_next_description') %></p>
  </div>

  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
      <div id="step-by-step-navigation" class="app-step-nav app-step-nav--large app-step-nav--active" data-show-text="Show" data-hide-text="Hide" data-show-all-text="Show all" data-hide-all-text="Hide all">
        <ol class="app-step-nav__steps">
          <li class="app-step-nav__step js-step step-is-shown" id="creating-your-contract">
            <div class="app-step-nav__header js-toggle-panel" data-position="1">
              <h2 class="app-step-nav__title">
              <span class="app-step-nav__circle app-step-nav__circle--number">
                <span class="app-step-nav__circle-inner">
                  <span class="app-step-nav__circle-background">
                    <span class="govuk-visually-hidden">Step</span> 1
                  </span>
                </span>
              </span>

                <span class="js-step-title"><div class="app-step-nav__button app-step-nav__button--title js-step-title-button" aria-expanded="true" aria-controls="step-panel-creating-your-contract-1"><span class="js-step-title-text">
                <%= t('.supplier_brief') %>
              </span><span class="app-step-nav__toggle-link js-toggle-link" aria-hidden="true" hidden="">Hide</span></div></span>
              </h2>
            </div>

            <div class="app-step-nav__panel js-panel" id="step-panel-creating-your-contract-1">
              <ol class="app-step-nav__list " data-length="3">
                <li class="app-step-nav__list-item js-list-item ">
                  <%= t('.supplier_brief_1') %>
                  <span data-position="1.1">
                  </span>
                </li>
                <li class="app-step-nav__list-item js-list-item ">
                  <span data-position="1.2"> <%= t('.supplier_brief_2') %>
                  </span>
                </li>
                <li class="app-step-nav__list-item js-list-item ">
                  <span data-position="1.2"> <%= t('.supplier_brief_3') %>
                  </span>
                </li>


              </ol>
            </div>
          </li>
          <li class="app-step-nav__step js-step step-is-shown" id="review-contract">
            <div class="app-step-nav__header js-toggle-panel" data-position="2">
              <h2 class="app-step-nav__title">
              <span class="app-step-nav__circle app-step-nav__circle--number">
                <span class="app-step-nav__circle-inner">
                  <span class="app-step-nav__circle-background">
                    <span class="govuk-visually-hidden">Step</span> 2
                  </span>
                </span>
              </span>

                <span class="js-step-title"><div class="app-step-nav__button app-step-nav__button--title js-step-title-button" aria-expanded="true" aria-controls="step-panel-review-contract-2"><span class="js-step-title-text">
                <%= t('.invitation_to_tender') %>
              </span><span class="app-step-nav__toggle-link js-toggle-link" aria-hidden="true" hidden="">Hide</span></div></span>
              </h2>
            </div>

            <div class="app-step-nav__panel js-panel" id="step-panel-review-contract-2">
              <ol class="app-step-nav__list " data-length="1">
                <li class="app-step-nav__list-item js-list-item ">
                  <span data-position="2.1"><%= t('.invitation_to_tender_1') %></span>
                </li>
                <li class="app-step-nav__list-item js-list-item ">
                  <span data-position="2.2"><%= t('.invitation_to_tender_2') %></span>
                </li>
                <li class="app-step-nav__list-item js-list-item ">
