<%= form_for @upload, html: { multipart: true } do |f| %>
  <div>
    <label>Current Accredited Suppliers</label>
    <p><%= link_to f.object.current_accredited_suppliers.file.filename, f.object.current_accredited_suppliers_url if f.object.current_accredited_suppliers.file %></p>
    <%= f.file_field :current_accredited_suppliers %>
    <%= f.hidden_field :current_accredited_suppliers_cache %>
  </div>
  <div>
    <label>Geographical Data All Suppliers</label>
    <p><%= link_to f.object.geographical_data_all_suppliers.file.filename, f.object.geographical_data_all_suppliers_url if f.object.geographical_data_all_suppliers.file %></p>
    <%= f.file_field :geographical_data_all_suppliers %>
    <%= f.hidden_field :geographical_data_all_suppliers_cache %>
  </div>
  <div>
    <label>Lot 1 and Lot 2 comparisons</label>
    <p><%= link_to f.object.lot_1_and_lot_2_comparisons.file.filename, f.object.lot_1_and_lot_2_comparisons_url if f.object.lot_1_and_lot_2_comparisons.file %></p>
    <%= f.file_field :lot_1_and_lot_2_comparisons %>
    <%= f.hidden_field :lot_1_and_lot_2_comparisons_cache %>
  </div>
  <div>
    <label>Master Vendor Contacts</label>
    <p><%= link_to f.object.master_vendor_contacts.file.filename, f.object.master_vendor_contacts_url if f.object.master_vendor_contacts.file %></p>
    <%= f.file_field :master_vendor_contacts %>
    <%= f.hidden_field :master_vendor_contacts_cache %>
  </div>
  <div>
    <label>Neutral Vendor Contacts</label>
    <p><%= link_to f.object.neutral_vendor_contacts.file.filename, f.object.neutral_vendor_contacts_url if f.object.neutral_vendor_contacts.file %></p>
    <%= f.file_field :neutral_vendor_contacts %>
    <%= f.hidden_field :neutral_vendor_contacts_cache %>
  </div>
  <div>
    <label>Pricing for tool</label>
    <p><%= link_to f.object.pricing_for_tool.file.filename, f.object.pricing_for_tool_url if f.object.pricing_for_tool.file %></p>
    <%= f.file_field :pricing_for_tool %>
    <%= f.hidden_field :pricing_for_tool_cache %>
  </div>
  <div>
    <label>Supplier lookup</label>
    <p><%= link_to f.object.supplier_lookup.file.filename, f.object.supplier_lookup_url if f.object.supplier_lookup.file %></p>
    <%= f.file_field :supplier_lookup %>
    <%= f.hidden_field :supplier_lookup_cache %>
  </div>
  <%= f.submit %>
<% end %>