<%= ccs_contact_us(
  Marketplace.support_form_link,
  having_problems_text: t('.contact_us.having_problems'),
  contact_us_link_text: t('.contact_us.contact_us'),
  contact_us_text: t('.contact_us.support')
) if Marketplace.support_form_link %>
<%= ccs_footer(
  meta: {
    items: [
      {
        text: t('layouts.application.cookie_policy_link.text'),
        href: cookie_policy_path,
        attributes: {
          aria: {
            label: t('layouts.application.cookie_policy_link.aria_label')
          }
        }
      },
      {
        text: t('layouts.application.cookie_settings_link.text'),
        href: cookie_settings_path,
        attributes: {
          aria: {
            label: t('layouts.application.cookie_settings_link.aria_label')
          }
        }
      },
      {
        text: t('layouts.application.privacy_link.text'),
        href: 'https://www.gov.uk/government/publications/crown-commercial-service-privacy-notice/crown-commercial-service-privacy-notice',
        attributes: {
          aria: {
            label: t('layouts.application.privacy_link.aria_label')
          }
        }
      },
      {
        text: t('layouts.application.accessibility_statement_link.text'),
        href: accessibility_statement_path,
        attributes: {
          aria: {
            label: t('layouts.application.accessibility_statement_link.aria_label')
          }
        }
      }
    ]
  }
) %>
