    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "ruby",
            "request": "attach",
            "name": "Attach",
            "port": 3000
        },
    {
        "type": "extensionHost",
        "request": "launch",
        "name": "Launch Extension",
        "runtimeExecutable": "${execPath}",
        "args": [
            "--extensionDevelopmentPath=${workspaceFolder}"
        ],
        "outFiles": [
            "${workspaceFolder}/out/**/*.js"
        ],
        "preLaunchTask": "npm"
    },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach",
            "port": 3000
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${file}"
        }
    ]
}